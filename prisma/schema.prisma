// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int @id @default(autoincrement())
  email    String @unique
  password String
  name     String?
  type     Int @default(2)
}

model Movie {
  id       Int @id @default(autoincrement())
  name     String
  imageUrl String
}